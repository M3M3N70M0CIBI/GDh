<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.GDh" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="background">@color/white</item>
        <item name="android:colorPrimary">@color/navBarDefCol</item>
        <item name="colorPrimaryVariant">@color/navBarDefCol</item>
        <item name="android:statusBarColor">@color/navBarDefCol</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowEnterTransition">@transition/trans_anim</item>
        <item name="android:windowExitTransition">@transition/trans_anim</item>
        <item name="android:textColor">@color/black</item>
